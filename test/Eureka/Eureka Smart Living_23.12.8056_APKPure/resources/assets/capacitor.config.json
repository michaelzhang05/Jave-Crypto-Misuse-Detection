{
	"appId": "me.eureka.esl.app",
	"appName": "Eureka Smart Living",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"DatePickerPlugin": {
			"mode": "dateAndTime",
			"locale": "pt_BR",
			"current": "Mon Mar 25 2024 15:16:04 GMT+0300 (Arabian Standard Time)",
			"format": "dd/MM/yyyy",
			"android": {
				"theme": "MyCustomeTheme"
			},
			"ios": {
				"style": "wheels"
			}
		}
	},
	"server": {
		"androidScheme": "http",
		"cleartext": true
	},
	"android": {
		"allowMixedContent": true
	}
}
